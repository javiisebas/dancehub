        "prettier": "^3.2.5",
        "prettier-plugin-organize-imports": "^4.3.0",
        "turbo": "^2.5.8"
    },
    "packageManager": "pnpm@8.15.5",
    "engines": {
        "node": ">=18"
    }
}
