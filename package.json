{
    "name": "my-turborepo",
    "version": "0.0.0",
    "private": true,
    "description": "",
    "author": "",
    "license": "UNLICENSED",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "test": "turbo run test",
        "test:e2e": "turbo run test:e2e",
        "lint": "turbo run lint",
        "format": "prettier --write --plugin=prettier-plugin-organize-imports \"**/*.{ts,tsx,md,json}\"",
