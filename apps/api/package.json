{
    "name": "api",
    "version": "0.0.0",
    "type": "commonjs",
    "private": true,
    "scripts": {
        "build": "nest build",
        "db:drop": "drizzle-kit drop",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:reset": "pnpm db:drop && pnpm db:generate && pnpm db:push && pnpm db:seed",
        "db:seed": "ts-node -r tsconfig-paths/register scripts/seed.ts",
        "db:studio": "drizzle-kit studio",
        "dev": "nest start --watch",
        "g": "node scripts/cli/generate.js",
        "generate": "node scripts/cli/generate.js",
        "health": "ts-node -r tsconfig-paths/register scripts/health-check.ts",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "setup": "bash scripts/setup-dev.sh",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main.js",
        "start:repl": "nest start --entryFile repl --watch",
        "start": "nest start",
        "storage:setup": "bash scripts/setup-storage.sh",
        "storage:verify": "ts-node -r tsconfig-paths/register scripts/verify-storage.ts",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:integration": "jest --testPathPattern=\\.integration\\.spec\\.ts$",
        "test:unit": "jest --testPathPattern=\\.spec\\.ts$",
        "test:watch": "jest --watch",
        "test": "jest",
        "tools": "bash scripts/dev-tools.sh",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.901.0",
        "@aws-sdk/s3-request-presigner": "^3.901.0",
        "@nestjs/axios": "^4.0.1",
        "@nestjs/bull": "^11.0.3",
        "@nestjs/cache-manager": "^2.3.0",
        "@nestjs/common": "^11.0.0",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.0",
        "@nestjs/cqrs": "^11.0.3",
        "@nestjs/event-emitter": "^2.1.0",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.0",
        "@nestjs/platform-socket.io": "^11.1.6",
        "@nestjs/schedule": "^6.0.1",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/websockets": "^11.1.6",
        "@repo/api": "workspace:*",
        "@repo/shared": "workspace:*",
        "@types/fluent-ffmpeg": "^2.1.27",
        "argon2": "^0.44.0",
        "body-parser": "^2.2.0",
        "bull": "^4.16.5",
        "cache-manager": "^5.7.6",
        "cache-manager-redis-yet": "^5.1.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "compression": "^1.8.1",
        "cookie-parser": "^1.4.7",
        "drizzle-orm": "^0.36.4",
        "fluent-ffmpeg": "^2.1.3",
        "handlebars": "^4.7.8",
        "helmet": "^8.1.0",
        "ioredis": "5.8.0",
        "multer": "^2.0.2",
        "nestjs-i18n": "^10.5.1",
        "nodemailer": "^7.0.6",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "postgres": "^3.4.5",
        "qrcode": "^1.5.4",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "sharp": "^0.34.4",
        "socket.io": "^4.8.1",
        "stripe": "^19.1.0",
        "uuid": "^13.0.0",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.0",
        "@repo/eslint-config": "workspace:*",
        "@repo/jest-config": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "@types/express": "^4.17.17",
        "@types/jest": "^30.0.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^22.10.7",
        "@types/nodemailer": "^7.0.2",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "^6.0.0",
        "axios": "^1.12.2",
        "dotenv": "^17.2.3",
        "drizzle-kit": "^0.28.1",
        "jest": "^29.7.0",
        "pg": "^8.16.3",
        "plop": "^4.0.4",
        "redis": "^5.8.3",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.5.4"
    }
}
